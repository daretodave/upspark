{
  "name": "Upspark",
  "version": "1.0.0",
  "main": "dist/main.js",
  "description": "A command runner for your desktop.",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "electron": "electron ./dist/main.js",
    "start": "electron-forge start",
    "package": "rimraf ./out && gulp deploy && electron-forge package",
    "make": "rimraf ./out && gulp deploy && electron-forge make"
  },
  "author": {
    "name": "David Rehmat",
    "email": "me@dave.blue",
    "url": "https://upspark.io/"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-env": "^1.2.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "electron": "^1.6.1",
    "electron-installer-windows": "^0.2.0",
    "electron-prebuilt-compile": "1.6.0",
    "file-loader": "^0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-run-electron": "^3.0.1",
    "gulp-util": "latest",
    "gutil": "^1.6.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.11.3",
    "raw-loader": "^0.5.1",
    "require-reload": "^0.2.2",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.4",
    "typescript": "2.1.1",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.5.4",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/compiler-cli": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular/upgrade": "2.4.1",
    "@exponent/electron-cookies": "^2.0.0",
    "angulartics2": "^1.4.3",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "bootstrap": "^3.3.7",
    "builtin-modules": "^1.1.1",
    "core-js": "^2.4.1",
    "debug": "^2.3.3",
    "electron-compile": "^6.1.2",
    "fs-promise": "^2.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-util": "^3.0.7",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.9",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "rxjs": "^5.0.2",
    "systemjs": "^0.19.41",
    "threads": "^0.7.2",
    "try-require": "^1.2.1",
    "uuid": "^3.0.1",
    "webpack": "^1.14.0",
    "zone.js": "^0.7.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg", "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "appCategoryType": "public.app-category.developer-tools",
        "prune": true,
        "ignore": [
          "_config",
          "_src",
          ".git",
          ".gitignore",
          ".idea",
          "LICENSE",
          "README",
          "tsconfig.json",
          "gulpfile",
          "^.*electron-packager.*$"
        ],
        "icon": "_src/static/icon/bulb.ico",
        "iconUrl": "https://s3.amazonaws.com/upspark/icon.png",
        "win32metadata": {
          "CompanyName": "Upspark",
          "FileDescription": "Upspark",
          "OriginalFilename": "Upspark.exe",
          "ProductName": "Upspark",
          "InternalName": "Upspark",
          "iconUrl": "https://s3.amazonaws.com/upspark/icon.png"
        }
      },
      "electronInstallerDMG": {
        "icon": "_src/static/icon/bulb.icns"
      },
      "electronWinstallerConfig": {
        "title": "Upspark",
        "name": "Upspark",
        "noMsi": true,
        "authors": "Upspark",
        "exe": "Upspark.exe",
        "ProductName": "Upspark",
        "setupIcon": "_src/static/icon/bulb.ico",
        "description": "Upspark",
        "version": "1.0.0",
        "iconUrl": "https://s3.amazonaws.com/upspark/icon.png",
        "version-string": {
          "CompanyName": "Upspark",
          "ProductName": "Upspark"
        }
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "David Rehmat",
        "name": "Upspark"
      },
      "windowsStoreConfig": {
        "packageName": "Upspark"
      }
    }
  }
}
