{
  "name": "upspark",
  "version": "0.0.1",
  "main": "dist/main.js",
  "description": "A command runner for your desktop.",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "electron": "electron ./dist/main.js",
    "build-win-32": "electron-packager ./dist upspark --platform win32 --arch ia32 --out ./dist --icon src/static/icon/bulb.ico",
    "build-win-64": "electron-packager ./dist upspark --platform win32 --arch x64 --out ./dist --icon src/static/icon/bulb.ico",
    "installer-win-32": "electron-installer-windows --src dist/upspark-win32-ia32/ --dest dist/installers/ia32/ --config config.json",
    "installer-win-64": "electron-installer-windows --src dist/upspark-win32-x64/ --dest dist/installers/x64/ --config config.json"
  },
  "author": {
    "name": "David Rehmat",
    "email": "me@dave.blue",
    "url": "https://upspark.io/"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.5.2",
    "file-loader": "^0.9.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-run-electron": "^3.0.1",
    "gulp-util": "latest",
    "gutil": "^1.6.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.11.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.4",
    "typescript": "2.1.1",
    "typings": "^1.4.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.5.4",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/compiler-cli": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "@angular/upgrade": "2.4.1",
    "@exponent/electron-cookies": "^2.0.0",
    "angulartics2": "^1.4.3",
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0",
    "bootstrap": "^3.3.7",
    "builtin-modules": "^1.1.1",
    "core-js": "^2.4.1",
    "debug": "^2.3.3",
    "electron": "^1.6.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-util": "^3.0.7",
    "jquery": "^3.1.1",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.2",
